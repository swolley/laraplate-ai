{
    "name": "swolley/laraplate-ai",
    "vendor": "swolley",
    "description": "AI Module contains all the necessary functionalities for build up a new AI integrated application",
    "version": "v1.2.0",
    "keywords": [
        "laravel",
        "laraplate",
        "ai"
    ],
    "authors": [
        {
            "name": "Swolley",
            "email": "myentity@inwind.it"
        }
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=8.5",
        "laravel/framework": "^12.0",
        "theodo-group/llphant": "^0.11.1"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "nunomaduro/phpinsights": "^2.13",
        "peckphp/peck": "^0.1.3",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-stressless": "^4.0",
        "pestphp/pest-plugin-type-coverage": "^4.0",
        "rector/rector": "^2.0"
    },
    "scripts": {
        "lint": [
            "rector",
            "pint --parallel",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:models -MN",
            "php artisan ide-helper:meta"
        ],
        "check": "vendor/bin/phpstan analyse --memory-limit=2G -v",
        "fix": "vendor/bin/phpstan analyse --memory-limit=2G -v --fix",
        "refactor": "rector",
        "test:type-coverage": "pest --type-coverage --min=100",
        "test:typos": "peck",
        "test:lint": [
            "pint --parallel --test",
            "rector --dry-run"
        ],
        "test:unit": "pest --parallel --coverage --exactly=100",
        "test:types": "phpstan",
        "test:refactor": "rector",
        "test": [
            "@test:type-coverage",
            "@test:unit",
            "@test:lint",
            "@test:types",
            "@test:refactor"
        ],
        "update:requirements": [
            "composer bump",
            "npx npm-check-updates -u"
        ],
        "version": "./scripts/version.sh --nointeractive",
        "version:silent": "./scripts/version.sh --silent --nointeractive",
        "version:major": "./scripts/version.sh major",
        "version:minor": "./scripts/version.sh minor",
        "version:patch": "./scripts/version.sh patch",
        "setup:hooks": "./scripts/setup-hooks.sh"
    },
    "extra": {
        "laravel": {
            "providers": [],
            "aliases": {}
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\AI\\": "app/",
            "Modules\\AI\\Database\\Factories\\": "database/factories/",
            "Modules\\AI\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\AI\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}